Local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "uDanasco Hub", HidePremium = false, SaveConfig = false, ConfigFolder = "uDanasco Hub"})

-- Guia Player
local TabPlayer = Window:MakeTab({
    Name = "Player",
    Icon = "rbxassetid://17340562200", -- Ícone de um boneco do Roblox
    PremiumOnly = false
})

local SpeedValue = 16 -- Velocidade padrão

local SpeedLabel = TabPlayer:AddLabel("Speed: " .. SpeedValue)

local IncreaseSpeedButton = TabPlayer:AddButton({
    Name = "+",
    Callback = function()
        SpeedValue = math.min(SpeedValue + 25, 400) -- Aumenta a velocidade em 25 até o máximo de 400
        SpeedLabel:Set("Speed: " .. SpeedValue)
    end    
})

local DecreaseSpeedButton = TabPlayer:AddButton({
    Name = "-",
    Callback = function()
        SpeedValue = math.max(SpeedValue - 25, 1) -- Reduz a velocidade em 25 até o mínimo de 1
        SpeedLabel:Set("Speed: " .. SpeedValue)
    end    
})

local SpeedButton = TabPlayer:AddButton({
    Name = "Apply Speed",
    Callback = function()
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = SpeedValue
    end    
})

local KillMobsButton = TabPlayer:AddButton({
    Name = "Kill Mobs",
    Callback = function()
        -- Função para modificar o NPC com o nome "mutant"
        local function ModificarMutants()
            -- Acessa todos os objetos do jogo
            local allObjects = game:GetDescendants()

            for _, obj in ipairs(allObjects) do
                if obj:IsA("Model") and obj.Name == "mutant" then
                    -- Renomeia o NPC para "Dyno"
                    obj.Name = "Dyno"

                    -- "Matar" o NPC (tornando-o invisível e imóvel)
                    local humanoid = obj:FindFirstChildOfClass("Humanoid")
                    if humanoid then
                        humanoid.Health = 0
                    end
                end
            end
        end

        -- Chama a função para modificar os NPCs com o nome "mutant"
        ModificarMutants()
    end
})

-- Guia Itens
local TabItens = Window:MakeTab({
    Name = "Itens",
    Icon = "rbxassetid://17340537625", -- Mesmo ícone da guia Player
    PremiumOnly = false
})

local function TeleportObject(objeto)
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

    if objeto.Name == "oildrum_low__0" then
        if objeto:IsA("Model") then
            local primaryPart = objeto.PrimaryPart
            if primaryPart and not primaryPart.Anchored then
                primaryPart.CFrame = CFrame.new(humanoidRootPart.Position)
            end
        elseif objeto:IsA("BasePart") and not objeto.Anchored then
            objeto.CFrame = CFrame.new(humanoidRootPart.Position)
        end
    end
end

local BarrilButton = TabItens:AddButton({
    Name = "Barril",
    Callback = function()
        -- Verificar objetos no Workspace
        for _, objeto in pairs(game.Workspace:GetDescendants()) do
            TeleportObject(objeto)
        end

        -- Verificar objetos em todo o jogo
        for _, objeto in pairs(game:GetDescendants()) do
            if objeto.Parent == nil then
                TeleportObject(objeto)
            end
        end    
    end
})

local CombustivelButton = TabItens:AddButton({
    Name = "Combustível",
    Callback = function()
        -- Função para teleportar objetos combustíveis
        local function TeleportarCombustiveis()
            -- Acessa todos os objetos do jogo
            local allObjects = game:GetDescendants()

            for _, obj in ipairs(allObjects) do
                local objName = obj.Name:lower()  -- Convertendo para minúsculas para comparar

                if obj:IsA("Model") and (objName == "gascan" or objName == "gascan") then
                    local mainPart = obj:FindFirstChild("main")
                    if mainPart then
                        mainPart.CFrame = CFrame.new(humanoidRootPart.Position)
                    end
                end
            end
        end

        -- Chama a função para teleportar objetos combustíveis
        TeleportarCombustiveis()
    end
})

