                        if primaryPart then
                            primaryPart.CFrame = CFrame.new(humanoidRootPart.Position)
                        end
                    end
                end
            end

            -- Chama a função para teleportar o item "Machado"
            TeleportarItem("Axe") -- Verifique o nome correto do item na hierarquia
        end

        -- Chama a função para teleportar o item "Machado"
        TeleportarMachado()
    end
})

