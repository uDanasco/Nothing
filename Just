local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "uDanasco Hub", HidePremium = false, SaveConfig = false, ConfigFolder = "uDanasco Hub"})

-- Guia Player
local TabPlayer = Window:MakeTab({
    Name = "Player",
    Icon = "rbxassetid://527994048", -- Ícone de um boneco do Roblox
    PremiumOnly = false
})

local SpeedValue = 16 -- Velocidade padrão

local SpeedLabel = TabPlayer:AddLabel("Speed: " .. SpeedValue)

local IncreaseSpeedButton = TabPlayer:AddButton({
    Name = "+",
    Callback = function()
        SpeedValue = math.min(SpeedValue + 25, 400) -- Aumenta a velocidade em 25 até o máximo de 400
        SpeedLabel:Set("Speed: " .. SpeedValue)
    end    
})

local DecreaseSpeedButton = TabPlayer:AddButton({
    Name = "-",
    Callback = function()
        SpeedValue = math.max(SpeedValue - 25, 1) -- Reduz a velocidade em 25 até o mínimo de 1
        SpeedLabel:Set("Speed: " .. SpeedValue)
    end    
})

local SpeedButton = TabPlayer:AddButton({
    Name = "Apply Speed",
    Callback = function()
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = SpeedValue
    end    
})

local KillMobsButton = TabPlayer:AddButton({
    Name = "Kill Mobs",
    Callback = function()
        -- Função para modificar o NPC com o nome "mutant"
        local function ModificarMutants()
            -- Acessa todos os objetos do jogo
            local allObjects = game:GetDescendants()

            for _, obj in ipairs(allObjects) do
                if obj:IsA("Model") and obj.Name == "mutant" then
                    -- Renomeia o NPC para "Dyno"
                    obj.Name = "Dyno"

                    -- "Matar" o NPC (tornando-o invisível e imóvel)
                    local humanoid = obj:FindFirstChildOfClass("Humanoid")
                    if humanoid then
                        humanoid.Health = 0
                    end
                end
            end
        end

        -- Chama a função para modificar os NPCs com o nome "mutant"
        ModificarMutants()
    end
})

-- Guia Itens
local TabItens = Window:MakeTab({
    Name = "Itens",
    Icon = "rbxassetid://527994048", -- Mesmo ícone da guia Player
    PremiumOnly = false
})

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

local function TeleportObject(objeto)
    if objeto.Name == "Object_2" and not objeto.Anchored then
        if objeto:IsA("Model") then
            local primaryPart = objeto.PrimaryPart
            if primaryPart then
                primaryPart.CFrame = CFrame.new(humanoidRootPart.Position)
            end
        elseif objeto:IsA("BasePart") then
            objeto.CFrame = CFrame.new(humanoidRootPart.Position)
        end
    end
end

local function CheckDescendants(parent)
    for _, objeto in pairs(parent:GetDescendants()) do
        TeleportObject(objeto)
    end
end

local VasoButton = TabItens:AddButton({
    Name = "Vaso",
    Callback = function()
        -- Verificar objetos no Workspace
        CheckDescendants(game.Workspace)

        -- Verificar objetos em todo o jogo
        for _, child in pairs(game:GetChildren()) do
            CheckDescendants(child)
        end
    end    
})

local BarrilButton = TabItens:AddButton({
    Name = "Barril",
    Callback = function()
        -- Função para teleportar barris
        local function TeleportarBarris()
            -- Acessa todos os objetos do jogo
            local allObjects = game:GetDescendants()

            for _, obj in ipairs(allObjects) do
                if obj:IsA("Model") and obj.Name == "oildrum_low__0" then
                    local primaryPart = obj.PrimaryPart
                    if primaryPart and not primaryPart.Anchored then
                        primaryPart.CFrame = CFrame.new(humanoidRootPart.Position)
                    end
                end
            end
        end

        -- Chama a função para teleportar barris
        TeleportarBarris()
    end    
})

local CombustivelButton = TabItens:AddButton({
    Name = "Combustível",
    Callback = function()
        -- Função para teleportar objetos combustíveis
        local function TeleportarCombustiveis()
            -- Acessa todos os objetos do jogo
            local allObjects = game:GetDescendants()

            for _, obj in ipairs(allObjects) do
                local objName = obj.Name:lower()  -- Convertendo para minúsculas para comparar

                if obj:IsA("Model") and (objName == "gascan" or objName == "gascan") then
                    local mainPart = obj:FindFirstChild("main")
                    if mainPart then
                        mainPart.CFrame = CFrame.new(humanoidRootPart.Position)
                    end
                end
            end
        end

        -- Chama a função para teleportar objetos combustíveis
        TeleportarCombustiveis()
    end
})

local TampasButton = TabItens:AddButton({
    Name = "Tampas",
    Callback = function()
        -- Função para teleportar tampas
        local function TeleportarTampas()
            -- Acessa todos os objetos do jogo
            local allObjects = game:GetDescendants()

            for _, obj in ipairs(allObjects) do
                if obj.Name == "BottleCap" or obj.Name == "BottleCapPile" then
                    if obj:IsA("Model") then
                        local primaryPart = obj.PrimaryPart
                        if primaryPart then
                            primaryPart.CFrame = CFrame.new(humanoidRootPart.Position)
                        end
                    elseif obj:IsA("BasePart") then
                        obj.CFrame = CFrame.new(humanoidRootPart.Position)
                    end
                end
            end
        end

        -- Chama a função para teleportar tampas
        TeleportarTampas()
    end
})

-- Criando a aba "Arma"
local TabArma = Window:MakeTab({
    Name = "Arma",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

-- Botão Pistola
TabArma:AddButton({
    Name = "Pistola",
    Callback = function()
        -- Código da Pistola aqui
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

        local function TeleportarHandle()
            -- Acessa todos os objetos do jogo
            local allObjects = game:GetDescendants()
        
            for _, obj in ipairs(allObjects) do
                if obj:IsA("Model") and obj.Name == "Pistol" then
                    local handle = obj:FindFirstChild("Handle")
                    if handle then
                        handle.CFrame = CFrame.new(humanoidRootPart.Position)
                    end
                end
            end
        end
        
        TeleportarHandle()
    end
})


-- Botão Mac10
TabArma:AddButton({
    Name = "Mac10",
    Callback = function()
        -- Código do Mac10 aqui
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

        local function TeleportarMac10()
            -- Função para teleportar o item "Mac10" para o jogador
            local function TeleportarItem(itemName)
                -- Acessa todos os objetos do jogo
                local allObjects = game:GetDescendants()
                
                for _, obj in ipairs(allObjects) do
                    if obj:IsA("Model") and obj.Name == itemName then
                        local primaryPart = obj.PrimaryPart
                        if primaryPart then
                            primaryPart.CFrame = CFrame.new(humanoidRootPart.Position)
                        end
                    end
                end
            end

            -- Chama a função para teleportar o item "Mac10"
            TeleportarItem("Mac10")
        end

        -- Chama a função para teleportar o item "Mac10"
        TeleportarMac10()
    end
})

-- Botão NoFog
local TabVisual = Window:MakeTab({
    Name = "Visual",
    Icon = "rbxassetid://527994048",
    PremiumOnly = false
})

TabVisual:AddButton({
    Name = "NoFog",
    Callback = function()
        -- Código para remover o fog (névoa)
        -- Obtém o serviço de renderização e a câmera do jogador
        local rs = game:GetService("RunService")
        local player = game.Players.LocalPlayer
        local camera = workspace.CurrentCamera

        -- Função para remover o efeito de neblina (fog)
        local function RemoverNeblina()
            camera.FarClipPlane = 99999  -- Define o plano de renderização longe para remover o neblina
        end

        -- Chama a função para remover o neblina quando o jogo é iniciado
        RemoverNeblina()

        -- Atualiza a remoção de neblina continuamente
        rs.RenderStepped:Connect(function()
            RemoverNeblina()
        end)
    end
})

-- Botão Esp
TabVisual:AddButton({
    Name = "Esp",
    Callback = function()
        -- Código para ativar o ESP
        -- Função para desenhar o ESP ao redor dos objetos especificados
        local function DesenharESP()
            -- Acessa todos os objetos do jogo
            local allObjects = game:GetDescendants()

            for _, obj in ipairs(allObjects) do
                local objName = obj.Name:lower()  -- Convertendo para minúsculas para comparar

                if objName == "bottlecap" or objName == "bottlecappile" or objName == "object_2" or objName == "oildrum_low__0" then
                    local objPosition, onScreen = workspace.CurrentCamera:WorldToViewportPoint(obj.Position)
                    
                    if onScreen then
                        local espSize = Vector3.new(100, 100, 100)  -- Tamanho da caixa do ESP
                        local espPosition = Vector2.new(objPosition.X, objPosition.Y)
                        
                        -- Desenha a caixa delimitadora
                        local espTopLeft = espPosition - Vector2.new(espSize.X / 2, espSize.Y / 2)
                        local espBottomRight = espPosition + Vector2.new(espSize.X / 2, espSize.Y / 2)
                        
                        local espBox = Drawing.new("Quad")
                        espBox.Visible = true
                        espBox.Color = Color3.fromRGB(255, 0, 0)  -- Cor vermelha
                        espBox.Thickness = 2
                        espBox.Filled = false
                        espBox.PointA = espTopLeft
                        espBox.PointB = Vector2.new(espBottomRight.X, espTopLeft.Y)
                        espBox.PointC = espBottomRight
                        espBox.PointD = Vector2.new(espTopLeft.X, espBottomRight.Y)
                    end
                end
            end
        end

        -- Atualiza o ESP continuamente
        game:GetService("RunService").RenderStepped:Connect(function()
            DesenharESP()
        end)
    end
})

OrionLib:Init()
