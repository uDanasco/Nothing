local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "uDanasco Hub", HidePremium = false, SaveConfig = false, ConfigFolder = "uDanasco Hub"})

-- Guia Player
local TabPlayer = Window:MakeTab({
    Name = "Player",
    Icon = "rbxassetid://527994048", -- Ícone de um boneco do Roblox
    PremiumOnly = false
})

local SpeedValue = 16 -- Velocidade padrão

local SpeedLabel = TabPlayer:AddLabel("Speed: " .. SpeedValue)

local IncreaseSpeedButton = TabPlayer:AddButton({
    Name = "+",
    Callback = function()
        SpeedValue = math.min(SpeedValue + 25, 400) -- Aumenta a velocidade em 25 até o máximo de 400
        SpeedLabel:Set("Speed: " .. SpeedValue)
    end    
})

local DecreaseSpeedButton = TabPlayer:AddButton({
    Name = "-",
    Callback = function()
        SpeedValue = math.max(SpeedValue - 25, 1) -- Reduz a velocidade em 25 até o mínimo de 1
        SpeedLabel:Set("Speed: " .. SpeedValue)
    end    
})

local SpeedButton = TabPlayer:AddButton({
    Name = "Apply Speed",
    Callback = function()
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = SpeedValue
    end    
})

local KillMobsButton = TabPlayer:AddButton({
    Name = "Kill Mobs",
    Callback = function()
        -- Função para modificar o NPC com o nome "mutant"
        local function ModificarMutants()
            -- Acessa todos os objetos do jogo
            local allObjects = game:GetDescendants()

            for _, obj in ipairs(allObjects) do
                if obj:IsA("Model") and obj.Name == "mutant" then
                    -- Renomeia o NPC para "Dyno"
                    obj.Name = "Dyno"

                    -- "Matar" o NPC (tornando-o invisível e imóvel)
                    local humanoid = obj:FindFirstChildOfClass("Humanoid")
                    if humanoid then
                        humanoid.Health = 0
                    end
                end
            end
        end

        -- Chama a função para modificar os NPCs com o nome "mutant"
        ModificarMutants()
    end
})

-- Guia Itens
local TabItens = Window:MakeTab({
    Name = "Itens",
    Icon = "rbxassetid://527994048", -- Mesmo ícone da guia Player
    PremiumOnly = false
})

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

local function TeleportObject(objeto)
    if objeto.Name == "Object_2" and not objeto.Anchored then
        if objeto:IsA("Model") then
            local primaryPart = objeto.PrimaryPart
            if primaryPart then
                primaryPart.CFrame = CFrame.new(humanoidRootPart.Position)
            end
        elseif objeto:IsA("BasePart") then
            objeto.CFrame = CFrame.new(humanoidRootPart.Position)
        end
    elseif objeto.Name == "oildrum_low__0" then
        if objeto:IsA("Model") then
            local primaryPart = objeto.PrimaryPart
            if primaryPart and not primaryPart.Anchored then
                primaryPart.CFrame = CFrame.new(humanoidRootPart.Position)
            end
        elseif objeto:IsA("BasePart") and not objeto.Anchored then
            objeto.CFrame = CFrame.new(humanoidRootPart.Position)
        end
    end
end

local function CheckDescendants(parent)
    for _, objeto in pairs(parent:GetDescendants()) do
        TeleportObject(objeto)
    end
end

local VasoButton = TabItens:AddButton({
    Name = "Vaso",
    Callback = function()
        -- Verificar objetos no Workspace
        CheckDescendants(game.Workspace)

        -- Verificar objetos em todo o jogo
        for _, child in pairs(game:GetChildren()) do
            CheckDescendants(child)
        end
    end    
})

local BarrilButton = TabItens:AddButton({
    Name = "Barril",
    Callback = function()
        -- Verificar objetos no Workspace
        CheckDescendants(game.Workspace)

        -- Verificar objetos em todo o jogo
        for _, child in pairs(game:GetChildren()) do
            CheckDescendants(child)
        end
    end    
})

local CombustivelButton = TabItens:AddButton({
    Name = "Combustível",
    Callback = function()
        -- Função para teleportar objetos combustíveis
        local function TeleportarCombustiveis()
            -- Acessa todos os objetos do jogo
            local allObjects = game:GetDescendants()

            for _, obj in ipairs(allObjects) do
                local objName = obj.Name:lower()  -- Convertendo para minúsculas para comparar

                if obj:IsA("Model") and (objName == "gascan" or objName == "gascan") then
                    local mainPart = obj:FindFirstChild("main")
                    if mainPart then
                        mainPart.CFrame = CFrame.new(humanoidRootPart.Position)
                    end
                end
            end
        end

        -- Chama a função para teleportar objetos combustíveis
        TeleportarCombustiveis()
    end
})

local TampasButton = TabItens:AddButton({
    Name = "Tampas",
    Callback = function()
        -- Função para teleportar tampas
        local function TeleportarTampas()
            -- Acessa todos os objetos do jogo
            local allObjects = game:GetDescendants()

            for _, obj in ipairs(allObjects) do
                if obj.Name == "BottleCap" or obj.Name == "BottleCapPile" then
                    if obj:IsA("Model") then
                        local primaryPart = obj.PrimaryPart
                        if primaryPart then
                            primaryPart.CFrame = CFrame.new(humanoidRootPart.Position)
                        end
                    elseif obj:IsA("BasePart") then
                        obj.CFrame = CFrame.new(humanoidRootPart.Position)
                    end
                end
            end
        end

        -- Chama a função para teleportar tampas
        TeleportarTampas()
    end
})
-- Criando a aba "Arma"
local TabArma = Window:MakeTab({
    Name = "Arma",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

-- Botão Pistola
TabArma:AddButton({
    Name = "Pistola",
    Callback = function()
        -- Código da Pistola aqui
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
    
        local function TeleportarHandle()
            -- Acessa todos os objetos do jogo
            local allObjects = game:GetDescendants()
        
            for _, obj in ipairs(allObjects) do
                if obj:IsA("Model") and obj.Name == "Pistol" then
                    local handle = obj:FindFirstChild("Handle")
                    if handle then
                        handle.CFrame = CFrame.new(humanoidRootPart.Position)
                    end
                end
            end
        end
        
        TeleportarHandle()
    end
})


-- Botão AK47
TabArma:AddButton({
    Name = "AK47",
    Callback = function()
        -- Código da AK47 aqui
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
    
        local function TeleportarHandle()
            -- Acessa todos os objetos do jogo
            local allObjects = game:GetDescendants()
        
            for _, obj in ipairs(allObjects) do
                if obj:IsA("Model") and obj.Name == "AK47" then
                    local handle = obj:FindFirstChild("Handle")
                    if handle then
                        handle.CFrame = CFrame.new(humanoidRootPart.Position)
                        break  -- Para a busca após encontrar o Handle
                    end
                end
            end
        end
        
        TeleportarHandle()
    end
})

-- Botão Machado
