-- Criar uma nova seção chamada Food
local TabFood = Window:MakeTab({
    Name = "Food",
    Icon = "rbxassetid://17340582473", -- Qualquer ícone desejado
    PremiumOnly = false
})

-- Botão Maçã
TabFood:AddButton({
    Name = "Maçã",
    Callback = function()
        -- Código da Maçã aqui
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

        local function TeleportarApple()
            local allObjects = game:GetDescendants()

            for _, obj in ipairs(allObjects) do
                local objName = obj.Name:lower()

                if objName == "apple" then
                    local handle = obj:FindFirstChild("Handle")
                    if handle then
                        handle.CFrame = CFrame.new(humanoidRootPart.Position)
                    end
                end
            end
        end

        TeleportarApple()
    end
})

-- Botão Banana
TabFood:AddButton({
    Name = "Banana",
    Callback = function()
        -- Código da Banana aqui
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

        local function TeleportarBanana()
            local allObjects = game:GetDescendants()

            for _, obj in ipairs(allObjects) do
                local objName = obj.Name:lower()

                if objName == "banana" and obj:IsA("Model") and obj:FindFirstChild("Banana") then
                    local banana = obj:FindFirstChild("Banana")
                    banana.CFrame = CFrame.new(humanoidRootPart.Position)
                end
            end
        end

        TeleportarBanana()
    end
})

-- Botão Donut
TabFood:AddButton({
    Name = "Donut",
    Callback = function()
        -- Código do Donut aqui
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

        local function TeleportarDonut()
            local allObjects = game:GetDescendants()

            for _, obj in ipairs(allObjects) do
                local objName = obj.Name:lower()

                if objName == "donut" then
                    local handle = obj:FindFirstChild("Handle")
                    if handle then
                        handle.CFrame = CFrame.new(humanoidRootPart.Position)
                    end
                end
            end
        end

        TeleportarDonut()
    end
})

-- Botão Pão
TabFood:AddButton({
    Name = "Pão",
    Callback = function()
        -- Código do Pão aqui
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

        local function TeleportarBread()
            local allObjects = game:GetDescendants()

            for _, obj in ipairs(allObjects) do
                local objName = obj.Name:lower()

                if objName == "bread" and obj:IsA("Model") and obj:FindFirstChild("Bread") then
                    local bread = obj:FindFirstChild("Bread")
                    bread.CFrame = CFrame.new(humanoidRootPart.Position)
                end
            end
        end

        TeleportarBread()
    end
})
